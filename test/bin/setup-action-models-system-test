#!/usr/bin/env bash

rake db:migrate VERSION=`(git ls-tree -r main --name-only db/migrate | sort | sed s/\\\//\\\n/g | tail -n1 | sed s/_/\\\n/ | head -n1)`
git add test/bin/setup-action-models-system-test
git checkout . 
git clean -d -f 

exit

rails g model Project team:references name:string
bin/super-scaffold crud Project Team name:text_field --sidebar=ti.ti-layout
rake db:migrate

bin/super-scaffold action-model:targets-many Archive Project Team
bin/super-scaffold action-model:targets-one Publish Project Team
bin/super-scaffold action-model:targets-one Generate Project Team
bin/super-scaffold action-model:performs-import Import Project Team
bin/super-scaffold action-model:performs-export Export Project Team

rake db:migrate
rails restart
